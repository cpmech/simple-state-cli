  "repository": "pedrosolabs/streamx-cli",
  "scripts": {
    "tsnode": "ts-node -O '{\"module\":\"commonjs\",\"resolveJsonModule\":true}'",
    "start": "yarn tsnode src/index.ts",
    "watch": "nodemon -e ts --exec yarn start",
    "build": "rm -rf dist && rollup --config rollup.config-cli.js && chmod +x dist/index.js",
    "test": "jest --verbose",
    "tw": "jest --watch --verbose",
    "cov": "yarn build && jest --coverage",
    "dist": "yarn build && yarn publish --access=public"
  },
  "description": "streamx-cli",
  "keywords": [
    "streamx-cli"
  ],
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "bin": {
    "streamx-cli": "./dist/index.js"
  },
  "devDependencies": {
    "@types/clui": "^0.3.0",
    "@types/configstore": "^4.0.0",
    "@types/figlet": "^1.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.18",
    "@types/minimist": "^1.2.0",
    "@types/mustache": "^0.8.32",
    "@types/node": "^12.7.5",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "rollup": "^1.21.4",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-insert": "^1.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript2": "^0.24.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@cpmech/basic": "^0.9.1",
    "@types/fs-extra": "^8.0.0",
    "chalk": "^2.4.2",
    "clui": "^0.3.6",
    "configstore": "^5.0.0",
    "figlet": "^1.2.4",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.0",
    "minimist": "^1.2.0",
    "mustache": "^3.1.0"
  }
